    "Mummy Lord": {
      "name": "Signore delle Mummie",
      "tokenName": "Signore delle Mummie",
      "description": "<p><em>Illustrazione del token di <a href=\"https://www.forgotten-adventures.net/\" target=\"_blank\" rel=\"noopener\">Forgotten Adventures</a>.</em></p>",
      "items": {
        "Legendary Resistance": {
          "name": "Resistenza Leggendaria",
          "description": "<p>Se l'[[lookup @name lowercase]] fallisce un tiro salvezza, può scegliere di superarlo.</p>",
