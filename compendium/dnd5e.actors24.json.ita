    "Drider": {
      "name": "Drider",
      "tokenName": "Drider",
      "description": "<p><em>Illustrazione del token di <a href=\"https://www.forgotten-adventures.net/\" target=\"_blank\" rel=\"noopener\">Forgotten Adventures</a>.</em></p>",
      "items": {
        "Spider Climb": {
          "name": "Scalata da Ragno",
          "description": "<p>L'[[lookup @name lowercase]] può scalare superfici difficili, anche lungo i soffitti, senza dover effettuare una prova di caratteristica.</p>"
