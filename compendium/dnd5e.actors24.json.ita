    "Mummy Lord": {
      "name": "Signore delle Mummie",
      "tokenName": "Signore delle Mummie",
      "description": "<p><em>Artwork del token di <a href=\"https://www.forgotten-adventures.net/\" target=\"_blank\" rel=\"noopener\">Forgotten Adventures</a>.</em></p>",
      "items": {
        "Legendary Resistance": {
          "name": "Resistenza Leggendaria",
          "description": "<p>Se il [[lookup @name lowercase]] fallisce un tiro salvezza, può invece scegliere di riuscirci.</p>",
